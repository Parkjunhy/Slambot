      </link>
      <link name="room_ne_top">
        <pose>3.25 4.5 0.75 0 0 0</pose>
        <collision name="room_ne_top_col"><geometry><box><size>2.5 0.1 1.5</size></box></geometry></collision>
        <visual    name="room_ne_top_vis"><geometry><box><size>2.5 0.1 1.5</size></box></geometry>
          <material><diffuse>0.9 0.85 0.6 1</diffuse></material></visual>
      </link>
      <link name="room_ne_bottom">
        <pose>3.35 0.5 0.75 0 0 0</pose>
        <collision name="room_ne_bottom_col"><geometry><box><size>1.7 0.1 1.5</size></box></geometry></collision>
        <visual    name="room_ne_bottom_vis"><geometry><box><size>1.7 0.1 1.5</size></box></geometry>
          <material><diffuse>0.9 0.85 0.6 1</diffuse></material></visual>
      </link>

      <!-- 기둥들 -->
      <link name="pillar_1">
        <pose>-3 -3 0.5 0 0 0</pose>
        <collision name="pillar_1_col"><geometry><box><size>0.3 0.3 1.0</size></box></geometry></collision>
        <visual    name="pillar_1_vis"><geometry><box><size>0.3 0.3 1.0</size></box></geometry>
          <material><diffuse>0.6 0.8 0.9 1</diffuse></material></visual>
      </link>
      <link name="pillar_2">
        <pose>3 -3 0.5 0 0 0</pose>

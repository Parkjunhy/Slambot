      <!-- 내부 가로 벽(원점 피해 y=1.0 유지) -->
      <link name="wall_h_left">
        <pose>-1.7 1.0 0.75 0 0 0</pose>
        <collision name="wall_h_left_col"><geometry><box><size>6.6 0.1 1.5</size></box></geometry></collision>
        <visual    name="wall_h_left_vis"><geometry><box><size>6.6 0.1 1.5</size></box></geometry>

      <link name="pillar_3">
        <pose>-3 3 0.5 0 0 0</pose>
        <collision name="pillar_3_col"><geometry><box><size>0.3 0.3 1.0</size></box></geometry></collision>
        <visual    name="pillar_3_vis"><geometry><box><size>0.3 0.3 1.0</size></box></geometry>
          <material><diffuse>0.6 0.9 0.6 1</diffuse></material></visual>
      </link>
    </model>

    <!-- GUI 카메라 -->
    <gui fullscreen="0">
      <camera name="user_camera">
        <pose>0 -12 6 0.5 0.3 0</pose>
      </camera>
    </gui>
  </world>
</sdf>
